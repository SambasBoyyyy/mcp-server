{
  "name": "stock-mcp-server",
  "version": "1.0.0",
  "description": "MCP Server for Stock Market Data",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:server": "node dist/server.js",
    "dev": "tsx src/index.ts",
